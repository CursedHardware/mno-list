{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "array",
  "items": {
    "$ref": "#/definitions/carrier"
  },
  "definitions": {
    "carrier": {
      "type": "object",
      "properties": {
        "canonical_id": {
          "type": "number",
          "description": "The unique identifier for the carrier"
        },
        "parent_canonical_id": {
          "type": "number",
          "description": "The parent carrier's unique identifier"
        },
        "carrier_name": {
          "type": "string",
          "description": "The name of the carrier"
        },
        "carrier_attribute": {
          "type": "array",
          "items": {
            "$ref": "#/definitions/carrier_attribute"
          }
        }
      },
      "additionalProperties": false,
      "required": [
        "canonical_id",
        "parent_canonical_id",
        "carrier_name",
        "carrier_attribute"
      ]
    },
    "carrier_attribute": {
      "type": "object",
      "properties": {
        "mccmnc_tuple": {
          "$ref": "#/definitions/string_array"
        },
        "imsi_prefix_xpattern": {
          "$ref": "#/definitions/string_array"
        },
        "spn": {
          "$ref": "#/definitions/string_array"
        },
        "plmn": {
          "$ref": "#/definitions/string_array"
        },
        "gid1": {
          "$ref": "#/definitions/string_array"
        },
        "gid2": {
          "$ref": "#/definitions/string_array"
        },
        "preferred_apn": {
          "$ref": "#/definitions/string_array"
        },
        "iccid_prefix": {
          "$ref": "#/definitions/string_array"
        },
        "privilege_access_rule": {
          "$ref": "#/definitions/string_array"
        }
      },
      "additionalProperties": false,
      "minProperties": 1
    },
    "string_array": {
      "type": "array",
      "items": {
        "type": "string"
      }
    }
  }
}